hi mkdURL       guibg=NONE guifg=${cyan}   gui=underline

" tpope/vim-fugitive
hi diffAdded   guibg=NONE guifg=${green} gui=NONE
hi diffRemoved guibg=NONE guifg=${red}   gui=NONE

" lewis6991/gitsigns.nvim
hi GitSignsAdd    guibg=NONE guifg=${green}  gui=NONE
hi GitSignsChange guibg=NONE guifg=${yellow} gui=NONE
hi GitSignsDelete guibg=NONE guifg=${red}    gui=NONE
hi GitSignsCurrentLineBlame guifg=${comment_grey}

" Git Highlighting
hi gitcommitComment       guibg=NONE guifg=${comment_grey} gui=NONE
hi gitcommitUnmerged      guibg=NONE guifg=${green}        gui=NONE
hi gitcommitOnBranch      guibg=NONE guifg=NONE            gui=NONE
hi gitcommitBranch        guibg=NONE guifg=${purple}       gui=NONE
hi gitcommitDiscardedType guibg=NONE guifg=${red}          gui=NONE
hi gitcommitSelectedType  guibg=NONE guifg=${green}        gui=NONE
hi gitcommitHeader        guibg=NONE guifg=NONE            gui=NONE
hi gitcommitUntrackedFile guibg=NONE guifg=${cyan}         gui=NONE
hi gitcommitDiscardedFile guibg=NONE guifg=${red}          gui=NONE
hi gitcommitSelectedFile  guibg=NONE guifg=${green}        gui=NONE
hi gitcommitUnmergedFile  guibg=NONE guifg=${yellow}       gui=NONE
hi gitcommitFile          guibg=NONE guifg=NONE            gui=NONE
hi gitcommitSummary       guibg=NONE guifg=${white}        gui=NONE
hi gitcommitOverflow      guibg=NONE guifg=${red}          gui=NONE
hi link gitcommitNoBranch       gitcommitBranch
hi link gitcommitUntracked      gitcommitComment
hi link gitcommitDiscarded      gitcommitComment
hi link gitcommitSelected       gitcommitComment
hi link gitcommitDiscardedArrow gitcommitDiscardedFile
hi link gitcommitSelectedArrow  gitcommitSelectedFile
hi link gitcommitUnmergedArrow  gitcommitUnmergedFile

